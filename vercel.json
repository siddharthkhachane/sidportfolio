{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "routes": [
    { 
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    { 
      "src": "^/favicon.ico",
      "dest": "/favicon.ico" 
    },
    { 
      "src": "^/manifest.json",
      "dest": "/manifest.json" 
    },
    { 
      "src": "^/robots.txt",
      "dest": "/robots.txt" 
    },
    { 
      "src": "^/asset-manifest.json",
      "dest": "/asset-manifest.json" 
    },
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    { 
      "src": ".*", 
      "dest": "/index.html" 
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
